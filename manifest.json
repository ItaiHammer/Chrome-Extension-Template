{
    "name": "Chrome Extension Template",
    "description": "description",
    "version": "0.1",
    "manifest_version": 2,

    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    
    "background": [
        {
            "scripts":["background.js"]
        }
    ]
}